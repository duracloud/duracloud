    <profiles>
        <profile>
            <id>profile-dev</id>
            <activation>
                <activeByDefault>true</activeByDefault>
                <property>
                    <name>profile</name>
                    <value>dev</value>
                </property>
            </activation>

            <properties>
                <tomcat.port>${tomcat.port.default}</tomcat.port>
                <database.home>${database.home.default}</database.home>
                <log.level>${log.level.default}</log.level>
            </properties>

            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.tomcat.maven</groupId>
                        <artifactId>tomcat7-maven-plugin</artifactId>
                        <version>2.2</version>
                        <executions>
                            <execution>
                                <id>tomcat-deploy</id>
                                <phase>pre-integration-test</phase>
                                <goals>
                                    <goal>deploy-only</goal>
                                </goals>
                                <configuration>
                                    <server>tomcat-server</server>
                                    <url>http://localhost:${tomcat.port}/manager/text</url>
                                    <update>true</update>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>

        </profile>

        <profile>
            <id>profile-bootstrap</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>profile</name>
                    <value>bootstrap</value>
                </property>
            </activation>

            <properties>
                <tomcat.port>${tomcat.port.default}</tomcat.port>
                <database.home>${database.home.default}</database.home>
            </properties>

            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-scm-plugin</artifactId>
                        <version>1.0</version>
                        <configuration>
                            <goals>compile</goals>
                            <connectionType>connection</connectionType>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>

        <profile>
            <id>profile-clover</id>

            <properties>
                <tomcat.port>${tomcat.port.default}</tomcat.port>
                <database.home>${database.home.default}</database.home>
            </properties>

            <build>
                <plugins>
                    <plugin>
                        <groupId>com.atlassian.maven.plugins</groupId>
                        <artifactId>maven-clover2-plugin</artifactId>
                        <version>3.1.8</version>
                        <configuration>
                            <licenseLocation>${cloverLicense}</licenseLocation>
                            <targetPercentage>50%</targetPercentage>
                            <includes>
                                <include>**/*.java</include>
                            </includes>
                            <generateHtml>true</generateHtml>
                            <generateHistorical>true</generateHistorical>
                            <historyDir>${user.home}/.clover/history/${project.artifact}</historyDir>
                            <jdk>1.7</jdk>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>

    </profiles>

    <properties>
        <tomcat.port.default>8080</tomcat.port.default>
        <database.home.default>/opt/derby/duracloudDB</database.home.default>
        <unitTests>**/*Test.class</unitTests>
        <integrationTests>**/Test*.class</integrationTests>
        <instrumentedTests>**/*Test*__*.class</instrumentedTests>
        <innerClasses>**/*$*</innerClasses>
        <log.level.default>INFO</log.level.default>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>

